.navbar-inverse
	background-color: rgba(0,0,0,0.8);
	height: 70px
	transition: all 0.3s ease;
	
	.navbar-header
		text-align: center
		
		.navbar-brand
			width: 150px
			padding: 10px
			color: #fff
			float: none
			display: inline-block
			
			img
				margin: 0 auto
				height: 50px;
				transition: all 0.3s ease;

		.navbar-toggle
			margin: 15px 35px 0 35px
			color: #fff
			border-color: #fff
			width: 40px
			height: 40px
			border-radius: 50px
			font-size: 18px
			position: relative
			transition: all 0.3s ease;
			
			i
				margin-top: 1px
				display: inline-block
		
		.navbar-toggle.active
			z-index: 1
			-webkit-filter: blur(0px);
			-moz-filter: blur(0px);
			-o-filter: blur(0px);
			-ms-filter: blur(0px);
			filter: blur(0px);
	
	.navbar-nav
		
		>li
			
			>a
				font-size: 16px
				text-transform: uppercase
				color: #fff
				padding: 0px 10px
				font-family: 'Lato-Semibold'
				height: 70px
				line-height: 70px
		>li.open
			>a
				background-color: transparent;
				&:focus
					background-color: transparent;
	
	.navbar-collapse
		border: none
		box-shadow: none
		text-align: center

.navbar-inverse.collapsed,
.navbar-inverse.collapsed .container
	height: 100%
	background-color: transparent

.navbar-inverse.collapsed .navbar-nav li a{
	height: 60px
	line-height: 60px
	display: inline-block
}

.transparent .navbar-brand img{
	margin-top: 20px !important;
	height: 74px !important;
}

.transparent{
	background-color: transparent; 
    border-color: transparent;
    height: 90px
}

.transparent .navbar-header .navbar-toggle{
	margin-top: 45px
	transform: scale(1.2)
}

.transparent .navbar-nav >li >a{
	height: 90px
	line-height: 90px
}

.navbar-inverse.collapsed .navbar-nav li a:before{
	z-index: 1;
    top: inherit;
    bottom: 10px;
}

