$menu_line = #4a4a4a;
$menu_line_1 = #195981;
$menu_line_2 = #d3a65d;
$menu_line_3 = #be1e2d;
$menu_line_4 = #00c89b;
$menu_line_5 = #d74ed0;
$menu_line_6 = #37b4ff;
$menu_line_7 = #78ee00;
$menu_line_8 = #373eff;

$menu_line_height = 3px;

vendor(prop, args)
	-webkit-{prop} args
	   -moz-{prop} args
	    -ms-{prop} args
	     -o-{prop} args
	        {prop} args

border-radius()
	vendor('border-radius', arguments)

box-shadow()
	vendor('box-shadow', arguments)

transition()
	vendor('transition', arguments)

transform()
	vendor('transform', arguments)

transform-origin()
	vendor('transform-origin', arguments)

user-select()
	vendor('user-select', arguments)

background-linear-gradient(startPoint, startColor, startInterval, endColor, endInterval, deprecatedWebkitStartPoint = false, deprecatedWebkitEndPoint = false)
	background-color startColor
	if deprecatedWebkitStartPoint && deprecatedWebkitEndPoint
		background-image -webkit-gradient(linear, deprecatedWebkitStartPoint, deprecatedWebkitEndPoint, color-stop(startInterval, startColor), color-stop(endInterval, endColor))
	background-image -webkit-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -moz-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -ms-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -o-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image linear-gradient(startPoint, startColor startInterval, endColor endInterval)

.navbar-fixed-top .navbar-collapse, 
.navbar-fixed-bottom .navbar-collapse
	max-height: calc(100% + 1px)
	height: calc(100% + 1px)
	
.navbar-inverse:before{ background-color: $menu_line }

.navbar-inverse .navbar-nav>li:nth-child(1)>a:before{ background-color: $menu_line_1 }
.navbar-inverse .navbar-nav>li:nth-child(2)>a:before{ background-color: $menu_line_2 }
.navbar-inverse .navbar-nav>li:nth-child(3)>a:before{ background-color: $menu_line_3 }
.navbar-inverse .navbar-nav>li:nth-child(4)>a:before{ background-color: $menu_line_4 }
.navbar-inverse .navbar-nav>li:nth-child(5)>a:before{ background-color: $menu_line_5 }
.navbar-inverse .navbar-nav>li:nth-child(6)>a:before{ background-color: $menu_line_6 }
.navbar-inverse .navbar-nav>li:nth-child(7)>a:before{ background-color: $menu_line_7 }
.navbar-inverse .navbar-nav>li:nth-child(8)>a:before{ background-color: $menu_line_8 }
.navbar-inverse .navbar-right>li:nth-child(1)>a:before{ background-color: $menu_line }

.navbar-inverse.collapsed .navbar-nav>li>a:before{
	height: 1px
}

.navbar-inverse:before,
.navbar-inverse .navbar-nav>li>a:before{
	content: '';
	display: block;
	position:absolute;
	top:0;
	left:0;
	width: 100%;
	z-index: -1;
	-webkit-transition: all .2s ease-in-out;
	-moz-transition: all .2s ease-in-out;
	-o-transition: all .2s ease-in-out;
	transition: all .2s ease-in-out;
}

.navbar-inverse:before,
.navbar-inverse .navbar-nav>li>a:before{
	height: $menu_line_height;
}

.navbar-inverse:not(.collapsed) .navbar-nav>li>a:hover:before,
.navbar-inverse:not(.collapsed) .navbar-nav>.active>a:before,
.navbar-inverse:not(.collapsed) .navbar-nav>.open>a:before{
	height: 100%;
}

.navbar-inverse .navbar-nav>.active>a, 
.navbar-inverse .navbar-nav>.active>a:hover, 
.navbar-inverse .navbar-nav>.active>a:focus{
	background-color: transparent;
}

.navbar-inverse .navbar-nav>.active>a:before, 
.navbar-inverse .navbar-nav>.active>a:hover:before, 
.navbar-inverse .navbar-nav>.active>a:focus:before{
	height: 100%;
}

html,body
